<h1>TASKLIST</h1>
<section>
  <nav>
    <p><strong> Add Task </strong></p>
    <p><strong> All Tasks </strong></p>
  </nav>
  <ol>
    <h2><strong>YOUR TASKS</strong></h2>
    <% @tasks.each do |task| %>
    <li><a href="/tasks/<%= task.id %>" name="<%= task.name%>"><%= task.name%></a></li>
    <!-- <li><strong><%= task.name%></strong></li> -->
    <p>Mark <%=task.completion_date%> | Edit | Delete </p>
    <% end %>
  </ol>
</section>
<footer>
  <p>copywrite 2017</p>
</footer>
